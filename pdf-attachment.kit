                                    <span class="program-video-availability-arrow"><i class="far fa-chevron-right"></i></span>
                                </a>
                            </div>
                            <!-- /.program-video-availability -->
                        </div>
                        <!-- /.program-video-content-nav -->

                        <div class="upload-attachment-big">
                            <div class="upload-attachment-title">Upload pdf file</div>
                            <div class="upload-file">
                                <template class="upload-file-template">
                                    <div class="upload-file-list-item">
                                        <a href="#" class="upload-file-list-item-remove"><i class="fal fa-times"></i></a>
                                        <div class="upload-file-list-item-icon"><i class="far fa-file"></i></div>
                                        <div class="upload-file-list-item-name"></div>
                                        <div class="upload-file-list-item-size"></div>
                                    </div>
                                </template>
                                <div class="upload-file-list" data-pull-list="attachment-pdf"></div>
                                <div class="upload-file-block">
                                    <input type="file" class="upload-file-input" data-pull="attachment-pdf" id="attachment-pdf" name="attachment-pdf" accept="application/pdf">
                                    <label class="upload-file-label" for="attachment-pdf">
                                        <span class="upload-file-label-wrap">
                                            <i class="far fa-plus"></i>
                                            <span class="upload-click">Click to upload</span>
                                        </span>
                                    </label>
                                </div>
                            </div>
                            <!-- /.upload-file -->
                        </div>
                        <!-- /.upload-attachment-big -->

                        <div class="input-wrapper">
                            <label for="program-pdf-link">Or provide a link</label>
                            <input type="text" class="input-text" id="program-pdf-link" name="program-pdf-link" placeholder="Link to file">
                            <div class="input-wrapper-info">
