                    </div>
                    <!-- /.program-video-caption -->
                    <div class="program-video-content tab">
                        <div class="input-wrapper">
                            <label for="program-pdf-name">Lesson name</label>
                            <input type="text" class="input-text symbols-maxlength" maxlength="150" placeholder="Enter name" id="program-pdf-name" name="program-pdf-name">
                            <div class="input-wrapper-info">
                                <div class="input-wrapper-error">Required field</div>
                                <div class="input-wrapper-symbols">
                                    <span class="symbols-maxlength-current">0</span>/150
                                </div>
                            </div>
                        </div>
                        <!-- /.input-wrapper -->
                        <div class="program-video-content-nav">
                            <div class="program-video-availability">
                                <a href="#" class="program-video-availability-button">
                                    <span class="program-video-availability-icon"><i class="far fa-lock-alt"></i></span>
